bool ronronar1() {
  if (digitalRead(buttonPin[0]) == HIGH) {
    if (lastbuttonstate[0] == LOW) {
      delay(50);
    } else {  // the button is being pressed
      buttonState[0] = HIGH;
    }
  } else {  //button pin is low
    buttonState[0] = LOW;
  }



  if (buttonState[0] == LOW && lastbuttonstate[0] == HIGH) {
    comeca = 0;
    loop1 = millis();
    stopPlayback();
  } else if (buttonState[0] == HIGH && comeca == 0) {
    comeca = 1;
    loop1 = millis();
    stopPlayback(); //se estiver a dar para
    Serial.println("ronrona1");
    startPlayback(sample, sizeof(sample));
  } else if (millis() - loop1 > audio) {  //audio lenght
    comeca = 0;
    //faz loop ao audio
  }

  lastbuttonstate[0] = digitalRead(buttonPin[0]);  //updates last button state
  return lastbuttonstate[0];
}




bool ronronar2() {

  if (digitalRead(buttonPin[1]) == HIGH) {
    if (lastbuttonstate[1] == LOW) {
      delay(50);
    } else {  // the button is being pressed
      buttonState[1] = HIGH;
    }
  } else {  //button pin is low
    buttonState[1] = LOW;
  }

  if (buttonState[1] == LOW) {
    loop2 = millis();
  }

  if (buttonState[1] == LOW && lastbuttonstate[1] == HIGH) {
    comeca1 = 0;
    loop2 = millis();
    foge = 0;
    stopPlayback();

  } else if (foge >= 6) {
    Serial.println("run");
    stopPlayback();
    right_meow = 0;
    foge = 0;
    startPlayback(meow, sizeof(meow));
    delay(50);
    Wire.beginTransmission(SLAVE_ADDR);
    Wire.write("X");
    Wire.endTransmission();
    delay(2000);
    delay(50);
    Wire.beginTransmission(SLAVE_ADDR);
    Wire.write("P");
    Wire.endTransmission();

  }
  else if (buttonState[1] == HIGH && comeca1 == 0) {
    Serial.println("ronrona2");
    comeca1 = 1;
    loop2 = millis();
    stopPlayback(); //se estiver a dar para
    startPlayback(sample, sizeof(sample));
  }
  else if (millis() - loop2 > audio && comeca1 == 1) { //audio lenght
    comeca1 = 0;
    foge++; //comea a contar quando vai fugir
    Serial.println(foge);
    //faz loop ao audio
  }

  lastbuttonstate[1] = digitalRead(buttonPin[1]);  //updates last button state
  return lastbuttonstate[1];
}



long measureDistance() {
  long duration, distance;

  digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);

  duration = pulseIn(echoPin, HIGH); //converte para int

  distance = duration / 58;
  Serial.print("distance measured:");
  Serial.println(distance);
  if (distance > 75) return 70;
  else return distance;
}

void switch_mode(int val) {
  Serial.println("switch");
  if (val == 'P') {
    current_mode = 0;
  }
  else if (val == 'W') {
    current_mode = 1;
  }
  else if (val == 'X') {
    current_mode = 2;
  }
  else if (val == 'Y') {
    current_mode = 3;
  }
  else if (val == 'Z') {
    Serial.println("sleep");
    current_mode = 4;
  }

}


